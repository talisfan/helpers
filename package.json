{
  "name": "init_node",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=.env.qa nodemon -r dotenv/config src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "start": "node dist/server.js ",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "prisma": "npx prisma generate && npx prisma generate --schema=./prisma-prices/prices.schema.prisma",
    "db:create": "npx prisma migrate dev --name init",
    "db:migrate": "npx prisma migrate dev --name $MIGRATION",
    "db:deploy": "npx prisma migrate deploy",
    "migration:create": "npm run typeorm migration:create -n ./src/database/migrations/$MIGRATION",
    "migration:up": "npm run typeorm migration:run -- -d ./src/database/ormconfig.ts",
    "migration:down": "npm run typeorm migration:revert -- -d ./src/database/ormconfig.ts"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@prisma/client": "^4.13.0",
    "aws-sdk": "^2.1348.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^11.6.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^9.0.0",
    "mime": "^2.5.2",
    "minio": "^7.0.33",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.14",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/minio": "^7.0.18",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
